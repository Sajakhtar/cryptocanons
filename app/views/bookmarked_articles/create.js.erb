const tweet_id = "<%= @bookmarked_article.tweet_id %>"

const replaceTweet = (tweet_id, outerHTML) => {
  const tweetCards = document.querySelectorAll('.card')
  tweetCards.forEach((tweetCard) => {
    const cardTweetId = tweetCard.dataset.tweetId
    if(cardTweetId === tweet_id) {
      console.log(tweetCard)
      tweetCard.outerHTML = outerHTML
    }
  })

}

replaceTweet(tweet_id, ' <%= j render partial: "shared/tweet_bookmark", locals: { tweet: @bookmarked_article } %>')
